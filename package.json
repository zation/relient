{
  "name": "relient",
  "version": "2.1.3",
  "author": "Liu Yang <zation1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "i18n"
  ],
  "description": "Relient is an util set working closely with Relient-Cli. It will be useful if your project is built on React, Redux, Redux-Form and of course Relient-Cli",
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zation/relient.git"
  },
  "dependencies": {
    "config": "^1.30.0",
    "date-fns": ">=2.0.0-alpha.7",
    "intl-messageformat-parser": ">=1.4.0",
    "lodash": ">=4.17.5",
    "normalizr": ">=3.2.4",
    "prop-types": ">=15.6.1",
    "query-string": ">=6.0.0",
    "react": ">=16.3.2",
    "react-redux": ">=5.0.7",
    "recompose": ">=0.26.0",
    "redux": ">=3.7.2",
    "redux-actions": ">=2.2.1",
    "redux-form": ">=7.3.0",
    "serialize-javascript": ">=1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-react": "7.0.0-beta.44",
    "@babel/preset-stage-2": "7.0.0-beta.44",
    "babel-eslint": "^8.2.3",
    "babel-plugin-lodash": "^3.3.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "fix": "yarn run lint --fix",
    "build": "rm -rf ./dist && mkdir ./dist && cp ./package.json ./README.md ./dist/ && babel ./src --out-dir ./dist --source-maps inline",
    "pub": "npm run build && npm publish ./dist"
  }
}
