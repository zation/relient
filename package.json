{
  "name": "relient",
  "version": "5.0.4",
  "author": "Liu Yang <zation1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "i18n"
  ],
  "description": "Relient is an util set working closely with Relient-Cli. It will be useful if your project is built on React, Redux and of course Relient-Cli",
  "engines": {
    "node": ">=10.0",
    "npm": ">=5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zation/relient.git"
  },
  "dependencies": {
    "@formatjs/icu-messageformat-parser": ">=1.1.4",
    "@types/config": "^0.0.38",
    "@types/history": "^4.7.7",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^17.0.3",
    "@types/redux-actions": "^2.6.1",
    "@types/serialize-javascript": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "bignumber.js": "^9.0.0",
    "config": ">=3.3.0",
    "date-fns": ">=2.19.0",
    "lodash": ">=4.17.19",
    "normalizr": ">=3.6.0",
    "query-string": ">=7.0.0",
    "react": ">=17.0.2",
    "redux": ">=4.0.5",
    "redux-actions": ">=2.6.5",
    "serialize-javascript": ">=5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.6",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore .",
    "fix": "npm run lint --fix",
    "build": "rm -rf ./dist && mkdir ./dist && cp ./package.json ./README.md ./dist/ && tsc && babel --config-file ./.babelrc.js ./dist --out-dir ./dist",
    "pub": "npm run lint && npm run build && npm publish ./dist --registry=https://registry.npmjs.org/",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  }
}
