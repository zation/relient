{
  "name": "relient",
  "version": "4.2.8",
  "author": "Liu Yang <zation1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "i18n"
  ],
  "description": "Relient is an util set working closely with Relient-Cli. It will be useful if your project is built on React, Redux and of course Relient-Cli",
  "engines": {
    "node": ">=10.0",
    "npm": ">=5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zation/relient.git"
  },
  "dependencies": {
    "config": ">=3.3.0",
    "date-fns": ">=2.11.0",
    "intl-messageformat-parser": ">=4.1.1",
    "lodash": ">=4.17.15",
    "query-string": ">=6.11.1",
    "react": ">=16.13.1",
    "redux": ">=4.0.5",
    "redux-actions": ">=2.6.5",
    "serialize-javascript": ">=3.0.0"
  },
  "devDependencies": {
    "@types/history": "^4.7.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.5",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.26",
    "@types/serialize-javascript": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-plugin-import": "^2.20.1",
    "final-form": "^4.18.7",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "normalizr": ">=3.6.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore './**/*.ts'",
    "fix": "yarn run lint --fix",
    "build": "rm -rf ./dist && mkdir ./dist && cp ./package.json ./README.md ./dist/ && tsc",
    "pub": "npm run build && npm publish ./dist --registry=https://registry.npmjs.org/"
  }
}
