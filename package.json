{
  "name": "relient",
  "version": "0.0.3",
  "engines": {
    "node": ">=8.9",
    "npm": ">=5.5"
  },
  "bin": {
    "relient": "./bin/relient.js"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.6",
    "browser-sync": "^2.18.13",
    "chokidar": "^1.7.0",
    "date-fns": "2.0.0-alpha.7",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.7.3",
    "normalizr": "^3.2.4",
    "pixrem": "^4.0.1",
    "pleeease-filters": "^4.0.0",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-import": "^11.0.0",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nested": "^2.1.2",
    "postcss-nesting": "^4.2.1",
    "postcss-selector-matches": "^3.0.1",
    "postcss-selector-not": "^3.0.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.1.1",
    "react-deep-force-update": "^1.1.1",
    "react-dev-utils": "^4.2.1",
    "react-error-overlay": "^3.0.0",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-form": "^7.1.2",
    "rimraf": "^2.6.2",
    "serialize-javascript": "^1.4.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.32",
    "@babel/core": "7.0.0-beta.32",
    "@babel/preset-env": "7.0.0-beta.32",
    "@babel/preset-react": "7.0.0-beta.32",
    "@babel/preset-stage-2": "7.0.0-beta.32",
    "babel-eslint": "^8.0.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-node": "^0.3.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint-js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js --fix",
    "fix-css": "yarn run lint-css --fix",
    "fix": "yarn run fix-js && yarn run fix-css",
    "build": "rm -rf ./dist && mkdir ./dist && cp ./package.json ./dist/ && babel ./src --out-dir ./dist --source-maps inline",
    "pub": "npm run build && npm publish ./dist"
  }
}
