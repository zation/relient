{
  "name": "relient",
  "version": "3.0.6",
  "author": "Liu Yang <zation1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "i18n"
  ],
  "description": "Relient is an util set working closely with Relient-Cli. It will be useful if your project is built on React, Redux, Redux-Form and of course Relient-Cli",
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zation/relient.git"
  },
  "dependencies": {
    "config": ">=3.2.2",
    "date-fns": ">=2.0.0-alpha.27",
    "intl-messageformat-parser": ">=3.0.7",
    "lodash": ">=4.17.15",
    "normalizr": ">=3.4.1",
    "prop-types": ">=15.7.2",
    "query-string": ">=6.8.2",
    "react": ">=16.9.0",
    "react-redux": ">=7.1.0",
    "recompose": ">=0.30.0",
    "redux": ">=4.0.4",
    "redux-actions": ">=2.6.5",
    "redux-form": ">=8.2.6",
    "serialize-javascript": ">=1.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "fix": "yarn run lint --fix",
    "build": "rm -rf ./dist && mkdir ./dist && cp ./package.json ./README.md ./dist/ && babel ./src --out-dir ./dist --source-maps inline",
    "pub": "npm run build && npm publish ./dist"
  }
}
