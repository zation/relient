{
  "name": "relient",
  "version": "4.3.0",
  "author": "Liu Yang <zation1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "i18n"
  ],
  "description": "Relient is an util set working closely with Relient-Cli. It will be useful if your project is built on React, Redux and of course Relient-Cli",
  "engines": {
    "node": ">=10.0",
    "npm": ">=5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zation/relient.git"
  },
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "config": ">=3.3.0",
    "date-fns": ">=2.15.0",
    "intl-messageformat-parser": ">=5.3.7",
    "lodash": ">=4.17.19",
    "normalizr": ">=3.6.0",
    "query-string": ">=6.13.1",
    "react": ">=16.13.1",
    "redux": ">=4.0.5",
    "redux-actions": ">=2.6.5",
    "serialize-javascript": ">=4.0.0"
  },
  "devDependencies": {
    "@types/history": "^4.7.7",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.44",
    "@types/redux-actions": "^2.6.1",
    "@types/serialize-javascript": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.22.0",
    "final-form": "^4.20.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore './**/*.ts'",
    "fix": "npm run lint --fix",
    "build": "rm -rf ./dist && mkdir ./dist && cp ./package.json ./README.md ./dist/ && tsc",
    "pub": "npm run lint && npm run build && npm publish ./dist --registry=https://registry.npmjs.org/"
  }
}
