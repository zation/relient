{
  "name": "relient",
  "version": "4.0.0",
  "author": "Liu Yang <zation1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "i18n"
  ],
  "description": "Relient is an util set working closely with Relient-Cli. It will be useful if your project is built on React, Redux and of course Relient-Cli",
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zation/relient.git"
  },
  "dependencies": {
    "config": ">=3.3.0",
    "date-fns": ">=2.11.0",
    "intl-messageformat-parser": ">=4.1.1",
    "lodash": ">=4.17.15",
    "normalizr": ">=3.6.0",
    "prop-types": ">=15.7.2",
    "query-string": ">=6.11.1",
    "react": ">=16.13.1",
    "react-redux": ">=7.2.0",
    "redux": ">=4.0.5",
    "redux-actions": ">=2.6.5",
    "serialize-javascript": ">=3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-sent": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "fix": "yarn run lint --fix",
    "build": "rm -rf ./dist && mkdir ./dist && cp ./package.json ./README.md ./dist/ && babel ./src --out-dir ./dist --source-maps inline",
    "pub": "npm run build && npm publish ./dist --registry=https://registry.npmjs.org/"
  }
}
